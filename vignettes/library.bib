file = {:Users/mak/Papers/Salmon{\_}2006{\_}Heterocyclic aromatic amines in domestically prepared chicken and fish from Singapore Chinese households.pdf:pdf},
isbn = {0278-6915},
issn = {02786915},
journal = {Food and Chemical Toxicology},
