file = {:Users/mak/Papers/Macaluso{\_}2003{\_}pRb2p130-E2F45-HDAC1-SUV39H1-p300 and pRb2p130-E2F45-HDAC1-SUV39H1-DNMT1 multimolecular complexes mediate the.pdf:pdf},
isbn = {0950-9232 (Print)$\backslash$r0950-9232 (Linking)},
issn = {09509232},
journal = {Oncogene},
keywords = {Breast carcinoma,Chromatin-modifying enzymes,Estrogen receptor-$\alpha$,pRb2/p130},
number = {23},
