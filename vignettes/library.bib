isbn = {1098-2272 (Electronic)$\backslash$n0741-0395 (Linking)},
issn = {07410395},
journal = {Genetic Epidemiology},
keywords = {GWAS,Genotype imputation,Mixture models},
number = {2},
pages = {102--110},
pmid = {21254217},
title = {{A comparison of approaches to account for uncertainty in analysis of imputed genotypes}},
volume = {35},
year = {2011}
}
@article{Wahlang2016,
abstract = {Exposure to persistent organic pollutants, including polychlorinated biphenyls (PCBs) is correlated with multiple vascular complications including endothelial cell dysfunction and atherosclerosis. PCB-induced activation of the vasculature subsequently leads to oxidative stress and induction of pro-inflammatory cytokines and adhesion proteins. Gene expression of these cytokines/proteins is known to be regulated by small, endogenous oligonucleotides known as microRNAs that interact with messenger RNA. MicroRNAs are an acknowledged component of the epigenome, but the role of environmentally-driven epigenetic changes such as toxicant-induced changes in microRNA profiles is currently understudied. The objective of this study was to determine the effects of PCB exposure on microRNA expression profile in primary human endothelial cells using the commercial PCB mixture Aroclor 1260. Samples were analyzed using Affymetrix GeneChip{\textregistered} miRNA 4.0 arrays for high throughput detection and selected microRNA gene expression was validated (RT-PCR). Microarray analysis identified 557 out of 6658 microRNAs that were changed with PCB exposure (p {\textless} 0.05). In-silico analysis using MetaCore database identified 21 of these microRNAs to be associated with vascular diseases. Further validation showed that Aroclor 1260 increased miR-21, miR-31, miR-126, miR-221 and miR-222 expression levels. Upregulated miR-21 has been reported in cardiac injury while miR-126 and miR-31 modulate inflammation. Our results demonstrated evidence of altered microRNA expression with PCB exposure, thus providing novel insights into mechanisms of PCB toxicity.},
author = {Wahlang, Banrida and Petriello, Michael C. and Perkins, Jordan T. and Shen, Shu and Hennig, Bernhard},
doi = {10.1016/j.tiv.2016.06.001},
issn = {18793177},
journal = {Toxicology in Vitro},
keywords = {Aroclor 1260,Endothelial cells,PCBs,Vascular diseases,miRNAs},
pages = {180--187},
pmid = {27288564},
title = {{Polychlorinated biphenyl exposure alters the expression profile of microRNAs associated with vascular diseases}},
type = {Journal Article},
url = {https://www.ncbi.nlm.nih.gov/pubmed/27288564},
volume = {35},
year = {2016}
}
