journal = {International journal of cancer},
keywords = {Adult,Aged,Alcohol Drinking,Animals,Breast Neoplasms,Breast Neoplasms: epidemiology,Breast Neoplasms: etiology,Breast Neoplasms: genetics,Case-Control Studies,Diet,Dietary Fats,Dietary Proteins,Female,France,Humans,Interviews as Topic,Menopause,Middle Aged,Multivariate Analysis,Risk Factors,Vitamins},
