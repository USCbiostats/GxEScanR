journal = {Diabetes care},
keywords = {Aged,Animals,C-Reactive Protein,C-Reactive Protein: metabolism,Diabetes Mellitus, Type 2,Diabetes Mellitus, Type 2: epidemiology,Diabetes Mellitus, Type 2: etiology,Eating,Female,Humans,Incidence,Male,Meat,Meat Products,Meat Products: adverse effects,Middle Aged,Netherlands,Netherlands: epidemiology,Poultry,Prospective Studies,Risk Factors},
