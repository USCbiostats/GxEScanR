doi = {10.1158/2159-8290.CD-17-1327},
file = {:Users/mak/Papers/Grasso{\_}2018{\_}Genetic mechanisms of immune evasion in colorectal cancer.pdf:pdf},
isbn = {2159-8290 (Electronic)2159-8274 (Linking)},
issn = {2159-8274},
