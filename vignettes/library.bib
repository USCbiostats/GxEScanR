@article{Brevik2011,
abstract = {Background—A diet high in red meat is an established colorectal cancer (CRC) risk factor. Carcinogens generated during meat cooking have been implicated as causal agents, and can induce oxidative DNA damage, which elicits repair by the base excision repair (BER) pathway. Methods—Using a family-based study we investigated the role of polymorphisms in four BER genes (APEX1 Gln51His, Asp148Glu; OGG1 Ser236Cys; PARP Val742Ala; XRCC1 Arg194Trp, Arg280His, Arg399Gln) as potential CRC risk factors and modifiers of the association between high-red meat or poultry diets and CRC risk. We tested for gene-environment interactions using case-only analyses (N = 577) and compared statistically significant results to those obtained using case-unaffected sibling comparisons (N = 307 sibships). Results—Carriers of the APEX1 codon 51 Gln/His genotype had a reduced CRC risk compared to carriers of the Gln/Gln genotype (OR 0.15, 95{\%} CI 0.03-0.69, p = 0.015). The association between higher red meat intake ({\textgreater}3 servings/week) and CRC was modified by the PARP Val762Ala SNP (case-only interaction p = 0.026). This SNP also modified the association between higher intake of high-temperature cooked red meat (case-only interaction p = 0.0009). Conclusions—We report evidence that the BER pathway PARP gene modifies the association of diets high in red meat cooked at high temperatures with risk of CRC. Impact—Our findings suggest a contribution to colorectal carcinogenesis of free radical damage as one of the possible harmful effects of a high-red meat diet.},
author = {Brevik, Asgeir and Joshi, Amit D and Corral, Rom{\'{a}}n and Onland-moret, N Charlotte and Kimberly, D and Marchand, Lo{\"{i}}c Le and Baron, John a and Martinez, Maria Elena and Haile, Robert W and Ahnen, Dennis J and Sandler, Robert S and Lance, Peter and Stern, Mariana C},
doi = {10.1158/1055-9965.EPI-10-0606.Polymorphisms},
file = {:Users/mak/Papers/Brevik{\_}2010{\_}Polymorphisms in Base Excision Repair Genes as Colorectal Cancer Risk Factors and Modifiers of the Effect of Diets.pdf:pdf},
issn = {1538-7755},
journal = {Cancer Epidemiol Biomarkers Prev.},
number = {12},
pages = {3167--3173},
pmid = {21037106},
title = {{Polymorphisms in base excision repair genes as colorectal cancer risk factors and modifiers of the effect of diets high in red meat}},
volume = {19},
year = {2011}
}
