@article{Jiao2013,
abstract = {Identification of gene-environment interaction (G × E) is important in understanding the etiology of complex diseases. However, partially due to the lack of power, there have been very few replicated G × E findings compared to the success in marginal association studies. The existing G × E testing methods mainly focus on improving the power for individual markers. In this paper, we took a different strategy and proposed a set-based gene-environment interaction test (SBERIA), which can improve the power by reducing the multiple testing burdens and aggregating signals within a set. The major challenge of the signal aggregation within a set is how to tell signals from noise and how to determine the direction of the signals. SBERIA takes advantage of the established correlation screening for G × E to guide the aggregation of genotypes within a marker set. The correlation screening has been shown to be an efficient way of selecting potential G × E candidate SNPs in case-control studies for complex diseases. Importantly, the correlation screening in case-control combined samples is independent of the interaction test. With this desirable feature, SBERIA maintains the correct type I error level and can be easily implemented in a regular logistic regression setting. We showed that SBERIA had higher power than benchmark methods in various simulation scenarios, both for common and rare variants. We also applied SBERIA to real genome-wide association studies (GWAS) data of 10,729 colorectal cancer cases and 13,328 controls and found evidence of interaction between the set of known colorectal cancer susceptibility loci and smoking.},
author = {Jiao, Shuo and Hsu, Li and B{\'{e}}zieau, St{\'{e}}phane and Brenner, Hermann and Chan, Andrew T. and Chang-Claude, Jenny and {Le Marchand}, Loic and Lemire, Mathieu and Newcomb, Polly A. and Slattery, Martha L. and Peters, Ulrike},
doi = {10.1002/gepi.21735},
file = {:Users/mak/Papers/Jiao{\_}2013{\_}SBERIA SetBased GeneEnvironment Interaction Test for Rare and Common Variants in Complex Diseases.pdf:pdf},
isbn = {1098-2272 (Electronic)$\backslash$r0741-0395 (Linking)},
issn = {07410395},
journal = {Genetic Epidemiology},
keywords = {Correlation screening,GWAS,Gene-environment interaction,Rare variants,Set based},
number = {5},
pages = {452--464},
pmid = {23720162},
title = {{SBERIA: Set-Based Gene-Environment Interaction Test for Rare and Common Variants in Complex Diseases}},
volume = {37},
year = {2013}
}
@article{Aulchenko2010,
abstract = {BACKGROUND: Over the last few years, genome-wide association (GWA) studies became a tool of choice for the identification of loci associated with complex traits. Currently, imputed single nucleotide polymorphisms (SNP) data are frequently used in GWA analyzes. Correct analysis of imputed data calls for the implementation of specific methods which take genotype imputation uncertainty into account.$\backslash$n$\backslash$nRESULTS: We developed the ProbABEL software package for the analysis of genome-wide imputed SNP data and quantitative, binary, and time-till-event outcomes under linear, logistic, and Cox proportional hazards models, respectively. For quantitative traits, the package also implements a fast two-step mixed model-based score test for association in samples with differential relationships, facilitating analysis in family-based studies, studies performed in human genetically isolated populations and outbred animal populations.$\backslash$n$\backslash$nCONCLUSIONS: ProbABEL package provides fast efficient way to analyze imputed data in genome-wide context and will facilitate future identification of complex trait loci.},
author = {Aulchenko, Yurii S. and Struchalin, Maksim V. and van Duijn, Cornelia M.},
doi = {10.1186/1471-2105-11-134},
file = {:Users/mak/Papers/Aulchenko{\_}2010{\_}ProbABEL package for genome-wide association analysis of imputed data.pdf:pdf},
isbn = {1471-2105 (Electronic)$\backslash$n1471-2105 (Linking)},
issn = {14712105},
journal = {BMC Bioinformatics},
pmid = {20233392},
title = {{ProbABEL package for genome-wide association analysis of imputed data}},
volume = {11},
year = {2010}
}
@article{John2005,
abstract = {Background: Migrant studies have shown that breast cancer risk increases$\backslash$nin women who move from countries with low incidence rates to countries$\backslash$nwith high rates. We examined the influence of migration history and$\backslash$nacculturation on breast cancer risk in Hispanic women ages 35 to 79$\backslash$nyears.$\backslash$nMethods: In a population-based case-control study conducted in the San$\backslash$nFrancisco Bay Area, information on migration history, language usage,$\backslash$nand other risk factors for breast cancer was collected through an$\backslash$nin-person interview for 991 cases and 1,285 controls.$\backslash$nResults: Breast cancer risk was 50{\%} lower in foreign-born Hispanics$\backslash$nthan U.S.-born Hispanics. Risk increased with increasing duration of$\backslash$nresidence in the United States, decreasing age at migration, and$\backslash$nincreasing acculturation. Among long-term foreign-born residents, risk$\backslash$nwas lower among Hispanics who moved to the United States at age {\textgreater}= 20$\backslash$nyears and those who spoke mostly Spanish. The difference in risk between$\backslash$nthird-generation or higher-generation Hispanics and recent migrants from$\backslash$nrural areas was similar to 6-fold in postmenopausal women and 4-fold in$\backslash$npremenopausal women. Adjustment for differences in the distribution of$\backslash$nbreast cancer risk factors greatly attenuated the associations with$\backslash$nmigration patterns in premenopausal women; reduced risks remained only$\backslash$nin those who resided in the United States for {\textless} 10 years or migrated at$\backslash$nage {\textgreater}= 30 years. In postmenopausal women, a 25{\%} to 30{\%} lower risk$\backslash$nremained among long-term residents and those who migrated to the United$\backslash$nStates before age 20 years.$\backslash$nConclusions: These findings suggest the importance of yet unidentified$\backslash$nprotective factors among both recent premenopausal migrants and$\backslash$npostmenopausal migrants.},
author = {John, Esther M. and Phipps, Amanda I. and Davis, Adam and Koo, Jocelyn},
doi = {10.1158/1055-9965.EPI-05-0483},
file = {:Users/mak/Papers/John{\_}2005{\_}Migration History, Acculturation, and Breast Cancer Risk in Hispanic Women.pdf:pdf},
isbn = {1055-9965 (Print)$\backslash$r1055-9965 (Linking)},
issn = {10559965},
journal = {Cancer Epidemiology Biomarkers and Prevention},
number = {12},
pages = {2905--2913},
pmid = {16365008},
title = {{Migration history, acculturation, and breast cancer risk in Hispanic women}},
type = {Journal Article},
volume = {14},
year = {2005}
