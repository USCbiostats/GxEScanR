doi = {10.1371/journal.pgen.1006296},
file = {:Users/mak/Papers/Gong{\_}2016{\_}Genome-Wide Interaction Analyses between Genetic Variants and Alcohol Consumption and Smoking for Risk of Colorect.PDF:PDF},
issn = {15537404},
journal = {PLoS Genetics},
