journal = {Mutation Research - Fundamental and Molecular Mechanisms of Mutagenesis},
keywords = {Breast cancer,Cigarettes,Oxidative stress,Survival,TLR,TNF,TRAF2,TRAIL},
pages = {19--28},
pmid = {25332681},
publisher = {Elsevier B.V.},
title = {{Diet and lifestyle factors modify immune/inflammation response genes to alter breast cancer risk and prognosis: The Breast Cancer Health Disparities Study}},
