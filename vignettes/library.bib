keywords = {Adult,Breast Neoplasms -- Epidemiology,Breast Neoplasms -- Risk Factors,Cell Surface -- Metabolism,Culture,Estrogens -- Metabolism,Female,Food Habits,Meat -- Adverse Effects,Middle Age,Premenopause,Progesterone -- Metabolism,Receptors,United States},
