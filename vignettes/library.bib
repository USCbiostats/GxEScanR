pmid = {17199431},
title = {{Estrogen Receptor (ER)- $\beta$ , But Not ER- $\alpha$ , Is Present in Thyroid Vessels: Immunohistochemical Evaluations in Multinodular Goiter and Papillary Thyroid Carcinoma}},
