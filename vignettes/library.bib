@article{Slattery2013a,
abstract = {Bone morphogenetic proteins (BMP) are thought to be important in breast cancer promotion and progression. We evaluated genetic variation in BMP-related genes and breast cancer risk among Hispanic (2,111 cases, 2,597 controls) and non-Hispanic White (NHW) (1,481 cases, 1,586 controls) women who participated in the 4-Corner's Breast Cancer Study, the Mexico Breast Cancer Study and the San Francisco Bay Area Breast Cancer Study. BMP genes and their receptors evaluated include ACVR1, AVCR2A, ACVR2B, ACVRL1, BMP1, BMP2, BMP4, BMP6, BMP7, BMPR1A, BMPR1B, BMPR2, MSTN and GDF10. Additionally, 104 ancestral informative markers were assessed to discriminate between European and native American ancestry. The importance of estrogen on BMP-related associations was suggested through unique associations by menopausal status and estrogen (ER) and progesterone (PR) receptor status of tumors. After adjustment for multiple comparisons ACVR1 (8 SNPs) was modestly associated with ER+PR+ tumors [odds ratios (ORs) between 1.18 and 1.39 padj {\textless} 0.05]. ACVR1 (3 SNPs) and BMP4 (3 SNPs) were associated with ER+PR- tumors (ORs 0.59-2.07; padj {\textless} 0.05). BMPR2 was associated with ER-PR+ tumors (OR 4.20; 95{\%} CI 1.62, 10.91; padj {\textless} 0.05) as was GDF10 (2 SNPs; ORs 3.62 and 3.85; padj {\textless} 0.05). After adjustment for multiple comparisons several SNPs remained associated with ER-PR- tumors (padj {\textless} 0.05) including ACVR1 BMP4 and GDF10 (ORs between 0.53 and 2.12). Differences in association also were observed by percentage of native ancestry and menopausal status. Results support the hypothesis that genetic variation in BMPs is associated with breast cancer in this admixed population.},
author = {Slattery, Martha L. and John, Esther M. and Torres-Mejia, Gabriela and Herrick, Jennifer S. and Giuliano, Anna R. and Baumgartner, Kathy B. and Hines, Lisa M. and Wolff, Roger K.},
doi = {10.1002/ijc.27960},
file = {:Users/mak/Papers/Slattery{\_}2013{\_}Genetic variation in bone morphogenetic proteins and breast cancer risk in hispanic and nonhispanic white women.pdf:pdf},
issn = {00207136},
journal = {International Journal of Cancer},
keywords = {ACVR1,BMP,BMPRIB,ER status,Hispanic,breast cancer,genetic admixture,survival},
number = {12},
pages = {2928--2939},
pmid = {23180569},
title = {{Genetic variation in bone morphogenetic proteins and breast cancer risk in hispanic and non-hispanic white women: The breast cancer health disparities study}},
volume = {132},
year = {2013}
}
