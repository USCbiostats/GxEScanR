journal = {Nutrition and Cancer},
keywords = {adult; article; breast cancer; caloric intake; can},
number = {September 2012},
pages = {37--41},
title = {{Nutrition and breast cancer risk by age 50: A population-based case-control study in Germany}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-0036954449{\&}partnerID=40{\&}md5=b5048fa9a4daa32b0db09ec41ab17226},
year = {2002}
}
@article{Madeleine2011,
abstract = {Proinflammatory cytokines are associated with age-related diseases including arthritis and heart disease. IL6 and TNF also play key roles in estrogen modulation in older women. We explored whether variation in IL6 and TNF genes influenced the risk of breast cancer in samples that differed by age group: ORs) were calculated to evaluate the risk associated with variants in IL6, IL6R, TNF, and TNFRSF1A. There was a significantly increased risk of breast cancer associated with one or more C{\{}{\textgreater}{\}}T alleles at IL6 rs2069861 among subjects in the oldest age group (OR 1.8, 95{\{}{\%}{\}} CI 1.1–2.9), but no overall increased risk of breast cancer associated with any IL6 or IL6R variants in the combined data. There were significantly elevated risks of breast cancer among women 45–64 years old associated with a UTR 5′ flanking SNP LTA rs2009658 C{\{}{\textgreater}{\}}G allele (OR 1.5, 95{\{}{\%}{\}} CI 1.1–1.9) and a nonsynonomous coding SNP TNFRSF1A rs767455 T{\{}{\textgreater}{\}}C allele (OR 1.3, 95{\{}{\%}{\}} CI 1.1–1.6); these two variants were also elevated in the combined data (OR 1.3, 95{\{}{\%}{\}} CI 1.1–1.5 and OR 1.2, 95{\{}{\%}{\}} CI 1.1–1.4, respectively). This study supports a modest association between a variant in IL6 and breast cancer among older women and TNF-related variants and breast cancer among middle-aged women. Further evaluation of these genes in other studies is warranted.},
author = {Madeleine, Margaret M. and Johnson, Lisa G. and Malkki, Mari and Resler, Alexa J. and Petersdorf, Effie W. and McKnight, Barbara and Malone, Kathleen E.},
doi = {10.1007/s10549-011-1520-4},
isbn = {0167-6806},
issn = {01676806},
journal = {Breast Cancer Research and Treatment},
keywords = {Breast cancer,Genetic variation,IL6,Proinflammatory cytokine genes,TNF},
number = {3},
pages = {887--899},
pmid = {21523452},
title = {{Genetic variation in proinflammatory cytokines IL6, IL6R, TNF-region, and TNFRSF1A and risk of breast cancer}},
type = {Journal Article},
url = {https://www.ncbi.nlm.nih.gov/pubmed/21523452},
volume = {129},
year = {2011}
}
@article{Badal2014,
abstract = {As one of the main extra-hepatic cytochrome P450 (CYP) enzymes, CYP1A1 has been comprehensively investigated for its ability to metabolize both exogenous and endogenous compounds into their carcinogenic derivatives. These derivatives are linked to cancer initiation and progression. The compound benzo-a-pyrene (BaP), a copious and noxious compound present in coal tar, automobile exhaust fumes, cigarette smoke and charbroiled food, is metabolised by CYP1A1 and has been studied in great detail. Other compounds reliant on the same enzyme for their activation include 7,12 dimethylbenz(a)anthracene (DMBA) and heterocyclic amine, 2-amino-1-methyl-6-phenylimidazo[4,5-b]pyridine (PhIP). This review takes an in-depth look at a number of phytochemicals, plant extracts and a few synthetic compounds that have been researched and deemed potential chemopreventives via their interaction with the activity and expression of CYP1A1. It will also review a useful active site model of CYP1A1. Based on inhibitors of CYP1A1 that have demonstrated in vivo use as chemopreventors, CYP1A1 is a useful initial target for screening compounds with such potential, with the use of rapid in vitro and/or in silico assessments. Chemoprevention is a means by which healthy tissues are protected via the prevention, inhibition or reversal of carcinogenesis. This review focuses on one important pathway of carcinogenesis and identifies the important role that CYP1A1 plays in that pathway. It is hoped that highlighting the importance of such a key target, will help revive further research into and application of inhibitors of CYP1A1 towards generating improved chemopreventors. Copyright {\textcopyright} 2014 John Wiley {\&} Sons, Ltd.},
author = {Badal, S. and Delgoda, R.},
doi = {10.1002/jat.2968},
file = {:Users/mak/Papers/Badal{\_}2014{\_}Role of the modulation of CYP1A1 expression and activity in chemoprevention.pdf:pdf},
isbn = {1099-1263 (Electronic)$\backslash$r0260-437X (Linking)},
issn = {10991263},
journal = {Journal of Applied Toxicology},
keywords = {Aryl hydrocarbon receptor,Aryl hydrocarbons,CYP1A1,Cancer,Chemoprevention},
number = {7},
pages = {743--753},
pmid = {24532440},
title = {{Role of the modulation of CYP1A1 expression and activity in chemoprevention}},
volume = {34},
year = {2014}
}
