arxivId = {NIHMS150003},
author = {Cotterchio, Michelle and Boucher, Beatrice A. and Manno, Michael and Gallinger, Steven and Okey, Allan B. and Harper, Patricia A.},
doi = {10.1158/1055-9965.EPI-08-0341},
eprint = {NIHMS150003},
file = {:Users/mak/Papers/Cotterchio{\_}2008{\_}Red Meat Intake, Doneness, Polymorphisms in Genes that Encode Carcinogen-Metabolizing Enzymes, and Colorectal Canc.pdf:pdf},
isbn = {2156623929},
issn = {10559965},
journal = {Cancer Epidemiology Biomarkers and Prevention},
