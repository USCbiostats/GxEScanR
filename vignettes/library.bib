issn = {0959437X},
journal = {Current Opinion in Genetics and Development},
number = {1},
pages = {3--10},
pmid = {18325755},
title = {{Pathways connecting inflammation and cancer}},
type = {Journal Article},
url = {http://www.sciencedirect.com/science/article/pii/S0959437X08000075 http://ac.els-cdn.com/S0959437X08000075/1-s2.0-S0959437X08000075-main.pdf?{\_}tid=1256c656-5aec-11e6-9e0e-00000aab0f01{\&}acdnat=1470388244{\_}253a3a15c63f62e48e5c35e325ffea85},
