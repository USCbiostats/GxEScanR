journal = {Cancer causes {\&} control : CCC},
keywords = {brain-computer communication,brain-computer interfaces,complete,consciousness,locked-in state},
month = {may},
number = {4},
pages = {391--401},
pmid = {18080775},
title = {{Reproductive history in relation to breast cancer risk among Hispanic and non-Hispanic white women.}},
