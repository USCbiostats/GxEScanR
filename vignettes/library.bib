issn = {01635581},
journal = {Nutrition and Cancer},
keywords = {Adult,Aged,Breast Neoplasms,Carotenoids,Case-Control Studies,Confidence Intervals,Diet,Diet Surveys,Dietary Fats, Unsaturated,Dietary Fiber,Female,Food Habits,Humans,Menopause,Mexico,Middle Aged,Odds Ratio,Risk Factors,Surveys and Questionnaires,Vitamin E,administration {\&} dosage,epidemiology,etiology,prevention {\&} control},
