doi = {10.1093/hmg/dds381},
file = {:Users/mak/Papers/Siddiq{\_}2012{\_}A meta-analysis of genome-wide association studies of breast cancer identifies two novel susceptibility loci at 6q.pdf:pdf},
isbn = {1460-2083 (Electronic)$\backslash$r0964-6906 (Linking)},
