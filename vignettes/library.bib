number = {2},
pages = {271--290},
pmid = {20640864},
title = {{Physiology and pathophysiology of matrix metalloproteases}},
type = {Journal Article},
url = {https://www.ncbi.nlm.nih.gov/pubmed/20640864},
volume = {41},
year = {2011}
}
